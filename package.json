{
  "name": "js-fullstack-starter",
  "version": "1.0.0",
  "description": "基于 React, Beidou 等技术的 JS 全栈开发项目模板",
  "scripts": {
    "start": "beidou start --workers=1 --port=1024",
    "debug": "beidou debug",
    "stop": "beidou stop",
    "dev": "beidou dev --port=1024",
    "build": "rimraf build && beidou build",
    "build:node": "beidou build --target=node",
    "test": "jest --no-color --no-cache --runInBand"
  },
  "dependencies": {
    "antd": "^4.0.0",
    "babel-polyfill": "^6.26.0",
    "beidou": "^2.2.1",
    "beidou-cli": "^2.1.1",
    "egg-mysql": "^3.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "babel-jest": "^25.1.0",
    "babel-plugin-import": "^1.13.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "jest-puppeteer": "^4.4.0",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.3.12",
    "rimraf": "^3.0.2"
  }
}
