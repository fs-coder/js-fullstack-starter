{
  "name": "egg-react-ssr-starter",
  "version": "1.0.0",
  "description": "A quick starter for server-rendered React apps",
  "scripts": {
    "start": "beidou start --workers=1 --port=1024",
    "debug": "beidou debug",
    "stop": "beidou stop",
    "dev": "beidou dev --port=1024",
    "build": "rimraf build && beidou build",
    "build:node": "beidou build --target=node"
  },
  "dependencies": {
    "antd": "^4.0.0",
    "babel-polyfill": "^6.26.0",
    "beidou": "^2.2.1",
    "beidou-cli": "^2.1.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.13.0",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.3.12",
    "rimraf": "^3.0.2"
  }
}
